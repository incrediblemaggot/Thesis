@article{goodbrod_effects_1990,
	title = {Effects of {larval} {population} {density} on {rates} of {development} and {interactions} {between} {two} {species} of \textit{Chrysomya} ({Diptera}: {Calliphoridae}) in {laboratory} {culture}},
