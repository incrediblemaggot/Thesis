	keywords = {Animal, Cadaver, Carcass, Detritus, ecology, Nutrient cycling, pdf 277, Plant Sciences, Spatial, Temporal},
	pages = {761--772},
	file = {Full Text PDF:C\:\\Users\\Admin\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\8ccfz8yp.default\\zotero\\storage\\S3XHAVXV\\Barton et al. - 2013 - The role of carrion in maintaining biodiversity an.pdf:application/pdf;Snapshot:C\:\\Users\\Admin\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\8ccfz8yp.default\\zotero\\storage\\K254K2D8\\s00442-012-2460-3.html:text/html}
