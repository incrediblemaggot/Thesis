@article{kao_collective_2014,
	title = {Collective {Learning} and {Optimal} {Consensus} {Decisions} in {Social} {Animal} {Groups}},
	volume = {10},
	abstract = {Author Summary Learning is ubiquitous among animal species, allowing individuals to adjust their behavior in response to their environment to improve their chances of survival and reproduction. However, while many animals live and make decisions within social groups, it is not well understood how associative learning functions within a social context. We describe an empirically derived model of collective learning and compare the learned performance of animals within groups to the optimal behavior for a wide range of environmental conditions and group sizes. We find that the learning rules derived from experiments with individual animals readily generalize to a social context, and these relatively simple rules result in behavior that is close to optimal, even when individuals know neither the size of their group nor the properties of environmental cues. Individuals that learn in isolation and subsequently join together as a group make substantially worse decisions. These results demonstrate the importance of learning within a collective context and highlight the need for experimental work to investigate the role of collective learning in enhancing decision accuracy in animal groups.},
	number = {8},
	urldate = {2015-02-13},
	journal = {PLoS Comput Biol},
	author = {Kao, Albert B. and Miller, Noam and Torney, Colin and Hartnett, Andrew and Couzin, Iain D.},
	month = aug,
